{
  "name": "horsax",
  "version": "1.0.0",
  "description": "TDD with express",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --watch",
    "dev": "cross-env NODE_ENV=development nodemon .",
    "start": "cross-env NODE_ENV=development node .",
    "lint": "eslint . ",
    "lintfix": "eslint . --fix"
  },
  "keywords": [
    "TDD",
    "express",
    "horsax",
    "test driven development"
  ],
  "author": "Abdallah Zidan",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "morgan": "^1.10.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.5",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": "warn",
      "eqeqeq": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "printWidth": 120
  }
}
